%table.table.table-striped
  %tr
    %th User
    %th Duration
    %th Date / Time
    %th Call Type
    %th From
    %th To
  - @logs.each do |log|
    %tr
      %td= log.user.name
      %td= "#{log.duration / 60} M : #{log.duration % 60} S"
      %td= log.created_at.strftime("%m-%d-%Y | %H:%m")
      %td= log.call_type
      %td= log.from
      %td= log.to

= paginate @logs
